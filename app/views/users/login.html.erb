<h1>Se connecter</h1>
<%= form_with url: create_session_path, local: true do |form| %>
  <div>
    <%= form.label :email %>
    <%= form.email_field :email %>
  </div>
  <div>
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>
  <div>
    <%= form.submit "Se connecter" %>
  </div>
<% end %>